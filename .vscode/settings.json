{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "*.inc": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "system_error": "cpp",
    "*.tcc": "cpp",
    "optional": "cpp",
    "ranges": "cpp"
  }
}